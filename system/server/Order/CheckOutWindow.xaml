<Window x:Class="server.Order.CheckOutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="CheckOutWindow" Height="408" Width="458" Loaded="Window_Loaded">
    <Grid>
        <Label Content="申请结账订单" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
        <ListView Height="244" HorizontalAlignment="Left" Margin="12,46,0,0" Name="UncheckList" VerticalAlignment="Top" Width="412" >
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="订单号" />
                    <GridViewColumn Header="桌台号" />
                    <GridViewColumn Header="应收款" />
                </GridView>
            </ListView.View>
        </ListView>
        <Button Content="结账" Height="53" HorizontalAlignment="Left" Margin="160,304,0,0" Name="enter" VerticalAlignment="Top" Width="120" />
    </Grid>
</Window>
